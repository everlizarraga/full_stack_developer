-var titulo = "Sub-titulo Principal "
-var titulos = ["Título Principal", "Subtítulo 1", "Subtítulo 2", "Subtítulo 3"]
-var enlaces = ['Acerca','Noticias','Contacto']

mixin caja(imagen,titulo,contenido)
    .caja 
        .caja__imagen: img(src="images/"+imagen, alt="img")
        .caja__contenido 
            h3=titulo 
            p=contenido 
            